{
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    },
    "csharp-ls": {
        "command": "csharp-ls",
        "filetypes": ["cs"],
        "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
      }
  },
  "snippets.ultisnips.pythonPrompt": false
}
